{
  "name": "erotas-console-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
    "freshInstall": "clear && rm -rf ./node_modules && rm -rf ./package-lock.json && npm install --package-lock-only && npm ci && npm run format",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:ci": "jest --ci --bail=10 --watchAll=false --runInBand --detectOpenHandles --testFailureExitCode --forceExit",
    "test:coverage:ci": "npm run test:ci -- --coverage --silent --verbose --coverageReporters=json-summary --runInBand --detectOpenHandles --forceExit"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/terminus": "^10.2.3",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "axios": "^1.4.0",
    "google-auth-library": "^9.15.1",
    "google-spreadsheet": "^4.1.4",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.13",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@azure/identity": "^3.2.3",
    "@azure/msal-node": "^1.18.0",
    "@microsoft/microsoft-graph-client": "^3.0.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.19",
    "@types/express": "^4.17.17",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^13.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "fs": "^0.0.1-security",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "setupFiles": ["<rootDir>/jestSetup.js"],
    "moduleFileExtensions": ["js", "json", "ts"],
    "coveragePathIgnorePatterns": [
      "src/lib",
      "src/types",
      "src/constants",
      "src/services/testingUtils"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
