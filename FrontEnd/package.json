{
  "name": "tough-nose-karate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "fix-line-endings": "powershell -Command \"Get-ChildItem -Recurse -Include '*.ts','*.tsx','*.js','*.jsx' | ForEach-Object { (Get-Content $_.FullName -Raw) -replace '`r`n', '`n' -replace '`r', '`n' | Set-Content $_.FullName -NoNewline }\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
